import java.util.*;

public class Note {

    public static void main(String[] args) {
		int[] ara = {1,2,3,4,5,6,7,8};
		int[] ara2 = {8,7,6,5,4,3,2,1};
		int[] input = new int[10];
		Scanner in = new Scanner(System.in);
			
		for(int i = 0 ; i < 8 ; i++){
			in.nextInt();
			input[i] = i;
		}
		
		for(int i = 0 ; i < 8 ; i++){
			System.out.println(input[i]);   
		}
    }

}
