#include <bits/stdc++.h>

using namespace std;

#define ll long long 

int main()
	{
	
	
		int n ; 
	

		cin >> n ; 
	
	
	
		while(n--){
		
			ll l ,  r ; 
		
			cin >> l >> r ; 
			int ans = l ;
			for(ll j = 1 ; l+j <= r ;j <<= 1){
			
				if(  (l & j) == 0 ) l+=j;
			
			
			}
		
		
			cout << l << endl; 
		
		
		}	
	
	
	

		return 0 ;
	}

